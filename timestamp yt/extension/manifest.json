{
  "manifest_version": 3,
  "name": "farm clipper",
  "version": "1.0.0",
  "description": "Will save timestamps to db",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["http://localhost:3000/"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/watch*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
